const byte LED = 11;
byte light_amount = 0;
byte modification = 5;
const byte delay_time = 30;
void setup() {
  Serial.begin(9600);
  randomSeed(analogRead(0));
  pinMode(LED, OUTPUT);
}
void loop() {
  analogWrite(LED, light_amount);
  modification = random(10);
  // dawdawdaw dafaut qutu explique truc
  light_amount = light_amount + modification;
  Serial.println(light_amount);
  if (light_amount <= 0 || light_amount >= 255) {
    modification = -modification;
  }
  delay(delay_time);
}
